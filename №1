#include<iostream>
#include<math.h>
using namespace std;
int main()
{
	double  x1, x2, y1, y2;
	cout << "Enter the coordinates of the first point: " << endl;
	cin >> x1 >> y1;
	cout << "Enter the coordinates of the second point: " << endl;
	cin >> x2 >> y2;
	cout << "distance between points is equal= " << sqrt(pow((x2 - x1), 2) + pow((y2 - y1), 2));
	return 0;


}
